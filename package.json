{
  "name": "tijaratk-api",
  "version": "0.0.1",
  "description": "",
  "author": "Safwat Fathi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "seed:dev": "NODE_ENV=development dotenv_config_path=.env.development ts-node -r tsconfig-paths/register -r dotenv/config ./src/common/seed.ts",
    "seed:prod": "NODE_ENV=production dotenv_config_path=.env.production ts-node -r tsconfig-paths/register -r dotenv/config ./src/common/seed.ts",
    "typeorm:dev": "NODE_ENV=development dotenv_config_path=.env.development ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=production dotenv_config_path=.env.production ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm:dev migration:create",
    "migration:generate:dev": "yarn typeorm:dev migration:generate -d src/config/orm.config.ts",
    "migration:run:dev": "yarn typeorm:dev migration:run -d src/config/orm.config.ts",
    "migration:revert:dev": "yarn typeorm:dev migration:revert -d src/config/orm.config.ts",
    "migration:generate:prod": "yarn typeorm:prod migration:generate -d src/config/orm.config.ts",
    "migration:run:prod": "yarn typeorm:prod migration:run -d src/config/orm.config.ts",
    "migration:revert:prod": "yarn typeorm:prod migration:revert -d src/config/orm.config.ts",
    "schema:drop:dev": "yarn typeorm:dev schema:drop -d src/config/orm.config.ts",
    "schema:drop:prod": "yarn typeorm:prod schema:drop -d src/config/orm.config.ts",
    "schema:sync:dev": "yarn typeorm:dev schema:sync -d src/config/orm.config.ts",
    "schema:sync:prod": "yarn typeorm:prod schema:sync -d src/config/orm.config.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/websockets": "^11.0.11",
    "class-transformer": "^0.5.1",
    "cron": "^3.5.0",
    "dotenv": "^16.4.7",
    "helmet": "^8.0.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/platform-ws": "^11.0.11",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/cache-manager": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "class-validator": "^0.14.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.11",
    "pg": "^8.13.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.1.3"
  }
}
